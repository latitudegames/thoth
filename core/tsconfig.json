{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strict": false,
    "rootDir": ".",
    "outDir": "./dist/",
    "declaration": true,
    "composite": true
  },
  "include": ["./"],
  "exclude": ["./node_modules/", "./dist/"]
}
